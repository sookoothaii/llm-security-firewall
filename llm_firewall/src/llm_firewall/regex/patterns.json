    {
      "name": "MIXED_SCRIPTS_LATIN_CYRILLIC",
      "pattern": "(?s)(?=.*\\p{Latin})(?=.*\\p{Cyrillic}).+",
      "flags": [
        "u"
      ],
      "weight": 1.6,
      "tags": [
        "homoglyph",
        "mixed_scripts"
