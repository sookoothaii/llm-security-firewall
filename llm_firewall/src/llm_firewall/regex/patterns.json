    },
    {
      "name": "SPECIAL_ENCODING_MENTIONS",
      "pattern": "(?i)\\b(?:base64|rot13|hex|url-?encoded|unicode\\s+escape)\\b",
      "flags": [
        "i"
      ],
      "weight": 0.7,
      "tags": [
        "obfuscation"
      ],
      "examples_positive": [
        "It's hex and base64 encoded."
      ],
      "examples_negative": [
        "It's hexadecimal notation in math."
      ]
    },
    {
